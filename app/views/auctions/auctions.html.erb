<h1> Hi, <%= current_user.username %></h1> <a href="/logout">Log Out</a>

<% @auctions.each do |auction| %>
	<div>
		<h2><%= auction.title %></h2>
		<div>
			<h3>Auction Description</h3>
			<p><%= auction.description %></p>
		<div>
		<div>
			<h3>Live On-Site Auction</h3>
			<p>Location: <%=auction.location%></p>
			<p>Begins at: <%= auction.start_time %></p>
			<p>Ends at: <%= auction.end_time %></p>
		</div>
		<div>
			<h3>Live On-site Preview</h3>
			<p><%= auction.preview %></p>
		</div>
	</div>
<% end %>